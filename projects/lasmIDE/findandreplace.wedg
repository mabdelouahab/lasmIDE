        .if rax
             mov   WFARBox        	,0
             mov   WFARBox        	,rv(gtk_builder_get_object, lbuilder,str$("WFARBox"))
            mov   WCckBRegular  ,rv(gtk_builder_get_object, lbuilder,str$("WCckBRegular"))
            mov	WCckBMatchBackW          	,rv(gtk_builder_get_object, lbuilder,str$("WCckBMatchBackW"))
            mov 	WCckBWarp         	,rv(gtk_builder_get_object, lbuilder,str$("WCckBWarp"))
            mov 	WCckBMatchWord    ,rv(gtk_builder_get_object, lbuilder,str$("WCckBMatchWord"))
            mov	WCckBMatchCase          	,rv(gtk_builder_get_object, lbuilder,str$("WCckBMatchCase"))
            mov 	WCBText          	,rv(gtk_builder_get_object, lbuilder,str$("CBSearchText"))
            mov	WCBReplaceW     ,rv(gtk_builder_get_object, lbuilder,str$("CBReplaceText"))
            mov 	WBtnFind         	,rv(gtk_builder_get_object, lbuilder,str$("WBtnFind1"))
            mov	WBtnFindAndReplace           	,rv(gtk_builder_get_object, lbuilder,str$("WBtnFindAndReplace1"))
            mov 	WBtnReplaceAll    ,rv(gtk_builder_get_object, lbuilder,str$("WBtnReplaceAll1"))
            mov 	WBtnClose         	,rv(gtk_builder_get_object, lbuilder,str$("WBtnClose1"))
		Notebook_Get_Current_Buffer proto
		mov buffer,rv(Notebook_Get_Current_Buffer)
	.if buffer
		.if rv(gtk_text_buffer_get_has_selection,buffer)
			gtk_text_buffer_get_selection_bounds(buffer,addr _search_IterST,addr _search_IterEN)
