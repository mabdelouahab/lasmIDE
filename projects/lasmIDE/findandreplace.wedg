			gtk_combo_box_text_insert_text(WCBText,0,rax)
			gtk_combo_box_set_active(WCBText,0)
		.endif          
	.endif
    
            invoke g_signal_connect_data,WBtnFind   			 ,str$("clicked")    ,addr On_WFARBox_WBtnFind_clicked    				,0,0,0     
             invoke g_signal_connect_data,WBtnFindAndReplace    ,str$("clicked")    	,addr On_WFARBox_WBtnFindAndReplace_clicked    	,1,0,0     ;On_WFARBox_WBtnFindAndReplace_clicked
             invoke g_signal_connect_data,WBtnReplaceAll    		,str$("clicked")    	,addr  On_WFARBox_WBtnReplaceAll_clicked   		,0,0,0     ;
